
import java.util.*;
 class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        boolean hasDuplicate=false;
        for(int i=0;i<n;i++){
            for(int j=1;j<n-1;j++){
                if(arr[i]==arr[j]){
                    hasDuplicate=true;
                }
                
            }
        }
        System.out.println(hasDuplicate);
        

    }
}